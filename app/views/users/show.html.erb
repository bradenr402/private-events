<h1>My Events</h1>
<ul>
  <% @user.created_events.each do |event| %>
    <li>
      <%= link_to event.title, event %>
    </li>
  <% end %>
</ul>
<%= link_to 'New Event', new_event_path %>

<h2>Events I'm Attending</h2>
<ul>
  <% @user.attended_events.each do |event| %>
    <li>
      <%= link_to event.title, event %>
    </li>
  <% end %>
</ul>
<%= link_to 'Back', events_path %>
