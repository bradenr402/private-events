<h1>New Event</h1>

<%= form_with model: @event do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :location %>
    <%= form.text_field :location %>
  </div>

  <div>
    <%= form.label :datetime %>
    <%= form.datetime_field :datetime %>
  </div>

  <div>
    <%= form.submit 'Create Event' %>
  </div>
<% end %>
